expose:
  ingress:
    {{/* annotations:
      kubernetes.io/tls-acme: "true" */}}
    hosts:
      core: harbor.{{ .lagoonBaseUrl }}
  {{/* tls:
    enabled: true
    certSource: secret
    secret:
      secretName: harbor-harbor-ingress */}}
externalURL: http://harbor.{{ .lagoonBaseUrl }}
harborAdminPassword: {{ .password }}
chartmuseum:
  enabled: false
clair:
  enabled: false
notary:
  enabled: false
trivy:
  enabled: false
jobservice:
  jobLogger: stdout
registry:
  replicas: 1
